//This program is a basic example of classes and objects in Java, showing how to define attributes, methods, create objects, and use them to represent employees.
package com.company;
 class Employee{
    public int id; // defining attributes
    public String name;
    public int sallery;

    // creating methods
     public void display(){
         System.out.println("my id is " + id + " and my name is " + name );
     }

     public int getSallery(){
         return sallery;
     }
}
public class custom_class {
     public static void main(String[] args) {
Employee obj = new Employee();
Employee obj1 = new Employee();

// setting attributes
obj.id = 56;
obj.name = "aman";
obj.sallery = 45000;

obj1.id = 12;
obj1.name = "surbhi";
obj1.sallery = 50000;

// printing attributes
//System.out.println(obj.id);
//System.out.println(obj.name);

 obj.display();
 int sallery = obj.getSallery();
 System.out.println(sallery);
 obj1.display();

    }
}
